	<osgi:service ref="testunitFlowCaseDetailService" interface="com.alibaba.testme.service.TestunitFlowCaseDetailService"></osgi:service>
	<osgi:service ref="testunitFlowDetailService" interface="com.alibaba.testme.service.TestunitFlowDetailService"></osgi:service>
	<osgi:service ref="testunitParamService" interface="com.alibaba.testme.service.TestunitParamService"></osgi:service>
	<osgi:service ref="testunitParamExtService" interface="com.alibaba.testme.service.TestunitParamExtService"></osgi:service>
	<osgi:service ref="workSpaceService" interface="com.alibaba.testme.service.WorkSpaceService"></osgi:service>
	
</beans>
