		        GMT_CREATE 
			   ,CREATOR 
			   ,GMT_MODIFIED 
			   ,MODIFIER 
			   ,VALUE_NAME 
			   ,VALUE 
			   ,TESTUNIT_PARAM_ID) VALUES (
			    now()
			   ,#creator# 
			   ,now()
			   ,#modifier# 
			   ,#valueName# 
			   ,#value# 
			   ,#testunitParamId#)
  
    <selectKey keyProperty="id" resultClass="Integer">
      SELECT LAST_INSERT_ID() AS value
    </selectKey>
  </insert>


  <update id="testunitParamExt.update" parameterClass="testunitParamExtDO">
    UPDATE tm_testunit_param_ext set GMT_MODIFIED=now()
