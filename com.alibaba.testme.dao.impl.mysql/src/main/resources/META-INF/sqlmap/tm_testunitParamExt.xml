        VALUE_NAME = #valueName#  
        ]]>  
      </isNotEmpty>
      <isNotEmpty property="value" prepend=",">
        <![CDATA[  
        VALUE = #value#  
        ]]>  
      </isNotEmpty>
      <isNotEmpty property="testunitParamId" prepend=",">
        <![CDATA[  
        TESTUNIT_PARAM_ID = #testunitParamId#  
        ]]>  
      </isNotEmpty>
    </dynamic>
     WHERE id = #id#  
  </update>

  <select id="testunitParamExt.findById" parameterClass="Integer" resultMap="testunitParamExtDO_resultMap">
