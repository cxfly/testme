<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="systemEnv">
<typeAlias alias="systemEnvDO"    type="com.alibaba.testme.domain.dataobject.SystemEnvDO" />

  <resultMap id="systemEnvDO_resultMap" class="systemEnvDO">
    <result property="id" column="ID" />
    <result property="gmtCreate" column="GMT_CREATE" />
    <result property="creator" column="CREATOR" />
    <result property="gmtModified" column="GMT_MODIFIED" />
    <result property="modifier" column="MODIFIER" />
    <result property="userId" column="USER_ID" />
    <result property="systemId" column="SYSTEM_ID" />
    <result property="configName" column="CONFIG_NAME" />
    <result property="isDefault" column="IS_DEFAULT" />
  </resultMap>
 

  <sql id="systemEnv.column_List">
		 ID
		 ,GMT_CREATE
		 ,CREATOR
		 ,GMT_MODIFIED
		 ,MODIFIER
		 ,USER_ID
		 ,SYSTEM_ID
		 ,CONFIG_NAME
		 ,IS_DEFAULT
  </sql>
   
  <sql id="systemEnv.queryCondition">
    <dynamic>
      <isNotEmpty property="id" prepend="AND">
        <![CDATA[  
        ID = #id#  
        ]]>  
      </isNotEmpty>
      <isNotEmpty property="creator" prepend="AND">
        <![CDATA[  
        CREATOR = #creator#  
        ]]>  
      </isNotEmpty>
      <isNotEmpty property="modifier" prepend="AND">
        <![CDATA[  
        MODIFIER = #modifier#  
        ]]>  
      </isNotEmpty>
      <isNotEmpty property="userId" prepend="AND">
        <![CDATA[  
        USER_ID = #userId#  
        ]]>  
      </isNotEmpty>
      <isNotEmpty property="systemId" prepend="AND">
        <![CDATA[  
        SYSTEM_ID = #systemId#  
        ]]>  
      </isNotEmpty>
      <isNotEmpty property="configName" prepend="AND">
        <![CDATA[  
        CONFIG_NAME = #configName#  
        ]]>  
      </isNotEmpty>
      <isNotEmpty property="isDefault" prepend="AND">
        <![CDATA[  
        IS_DEFAULT = #isDefault#  
        ]]>  
      </isNotEmpty>
    </dynamic>
  </sql>

  
  <insert id="systemEnv.add" parameterClass="systemEnvDO">
    INSERT INTO tm_system_env(
   				GMT_CREATE
   			   ,CREATOR
   			   ,GMT_MODIFIED
   			   ,MODIFIER
   			   ,USER_ID
   			   ,SYSTEM_ID
   			   ,CONFIG_NAME
   			   ,IS_DEFAULT) VALUES (
   	            now()
   	           ,#creator#
   	           ,now()
   	           ,#modifier#
   	           ,#userId#
   	           ,#systemId#
   	           ,#configName#
   	           ,#isDefault#)
  
    <selectKey keyProperty="id" resultClass="Integer">
      SELECT LAST_INSERT_ID() AS value
    </selectKey>
  
  </insert>


  <update id="systemEnv.update" parameterClass="systemEnvDO">
    UPDATE tm_system_env set GMT_MODIFIED = now()
    <dynamic>
      <isNotEmpty property="modifier" prepend=",">
        <![CDATA[  
        MODIFIER = #modifier#  
        ]]>  
      </isNotEmpty>
      <isNotEmpty property="userId" prepend=",">
        <![CDATA[  
        USER_ID = #userId#  
        ]]>  
      </isNotEmpty>
      <isNotEmpty property="systemId" prepend=",">
        <![CDATA[  
        SYSTEM_ID = #systemId#  
        ]]>  
      </isNotEmpty>
      <isNotEmpty property="configName" prepend=",">
        <![CDATA[  
        CONFIG_NAME = #configName#  
        ]]>  
      </isNotEmpty>
      <isNotEmpty property="isDefault" prepend=",">
        <![CDATA[  
        IS_DEFAULT = #isDefault#  
        ]]>  
      </isNotEmpty>
    </dynamic>
     WHERE id = #id#  
  </update>

  <select id="systemEnv.findById" parameterClass="Integer" resultMap="systemEnvDO_resultMap">
     SELECT 
    <include refid="systemEnv.column_List" />
    <![CDATA[  
     FROM tm_system_env
     WHERE id = #id#
    ]]>  
  </select>
  
   
  <delete id="systemEnv.deleteById" parameterClass="Integer">
    <![CDATA[  
     DELETE FROM tm_system_env 
      WHERE id = #id#
    ]]>  
  </delete>

  <select id="systemEnv.findList" resultMap="systemEnvDO_resultMap" parameterClass="systemEnvDO">
     SELECT 
    <include refid="systemEnv.column_List" />
     FROM tm_system_env
	WHERE 1=1
    <include refid="systemEnv.queryCondition" />
  </select>
  
</sqlMap>
