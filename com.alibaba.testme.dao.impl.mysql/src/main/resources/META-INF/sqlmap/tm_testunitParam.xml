  <insert id="testunitParam.add" parameterClass="testunitParamDO">
    INSERT INTO tm_testunit_param(
		        GMT_CREATE 
			   ,CREATOR 
			   ,GMT_MODIFIED 
			   ,MODIFIER 
			   ,LABEL_NAME 
			   ,PARAM_NAME 
			   ,FORM_CTRL_TYPE 
			   ,TESTUNIT_ID 
			   ,DEFAULT_VALUE 
			   ,RANK
			   ,IS_REQUIRED
			   ,HELP) VALUES (
			    now()
			   ,#creator# 
			   ,now()
			   ,#modifier# 
			   ,#labelName# 
			   ,#paramName# 
			   ,#formCtrlType# 
			   ,#testunitId# 
			   ,#defaultValue# 
			   ,#rank# 
			   ,#isRequired# 
			   ,#help#)
	
	<selectKey keyProperty="id" resultClass="Long">
      SELECT LAST_INSERT_ID() AS value
    </selectKey>
  </insert>


  <update id="testunitParam.update" parameterClass="testunitParamDO">
    UPDATE tm_testunit_param set GMT_MODIFIED=now()
    <dynamic>
      <isNotEmpty property="modifier" prepend=",">
        <![CDATA[  
        MODIFIER = #modifier#  
        ]]>  
      </isNotEmpty>
      <isNotEmpty property="labelName" prepend=",">
        <![CDATA[  
        LABEL_NAME = #labelName#  
        ]]>  
      </isNotEmpty>
      <isNotEmpty property="paramName" prepend=",">
        <![CDATA[  
        PARAM_NAME = #paramName#  
        ]]>  
      </isNotEmpty>
      <isNotEmpty property="formCtrlType" prepend=",">
        <![CDATA[  
        FORM_CTRL_TYPE = #formCtrlType#  
        ]]>  
      </isNotEmpty>
      <isNotEmpty property="testunitId" prepend=",">
