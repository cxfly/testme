      </isNotEmpty>
      <isNotEmpty property="help" prepend=",">
        <![CDATA[  
        HELP = #help#  
        ]]>  
      </isNotEmpty>
    </dynamic>
     WHERE id = #id#  
  </update>

  <select id="testunitParam.findById" parameterClass="Integer" resultMap="testunitParamDO_resultMap">
     SELECT 
    <include refid="testunitParam.column_List" />
    <![CDATA[  
     FROM tm_testunit_param
     WHERE id = #id#
    ]]>  
  </select>
  
   
  <delete id="testunitParam.deleteById" parameterClass="Integer">
    <![CDATA[  
     DELETE FROM tm_testunit_param 
      WHERE id = #id#
    ]]>  
  </delete>

  <select id="testunitParam.findList" resultMap="testunitParamDO_resultMap" parameterClass="testunitParamDO">
     SELECT 
    <include refid="testunitParam.column_List" />
     FROM tm_testunit_param
	WHERE 1=1
